{
  "name": "@fehnomenal/sveltekit-gen-routes",
  "version": "4.1.6",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./helpers": {
      "import": "./dist/helpers.js",
      "types": "./dist/helpers.d.ts"
    },
    "./types": {
      "types": "./dist/public-types.d.ts"
    }
  },
  "scripts": {
    "build:code": "bun build --define process.env.NODE_ENV=\"'production'\" --target=node --outdir=dist --splitting -e @sindresorhus/slugify -e typescript -e vite src/plugin/index.ts src/plugin/helpers.ts",
    "build:declarations": "tsc --project tsconfig.plugin.json",
    "build": "rm -rf dist && bun run build:code && bun run build:declarations",
    "dev": "vite dev",
    "build:e2e": "vite build",
    "test": "vitest",
    "check": "svelte-check --tsconfig tsconfig.json",
    "format": "prettier -w src",
    "changeset": "changeset",
    "version": "changeset version"
  },
  "dependencies": {
    "@sindresorhus/slugify": "2.2.1"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.1",
    "@fehnomenal/git-publisher": "github:fehnomenal/git-publisher#v1.0.3",
    "@sveltejs/kit": "2.5.18",
    "@testing-library/svelte": "5.2.0",
    "@types/bun": "latest",
    "happy-dom": "14.12.3",
    "prettier": "3.2.5",
    "svelte": "4.2.18",
    "svelte-check": "3.8.4",
    "typescript": "5.5.3",
    "vite": "5.3.4",
    "vitest": "2.0.3"
  },
  "peerDependencies": {
    "typescript": "*",
    "vite": "^5"
  },
  "files": [
    "./dist"
  ],
  "packageManager": "pnpm@9.14.1+sha512.7f1de9cffea40ff4594c48a94776112a0db325e81fb18a9400362ff7b7247f4fbd76c3011611c9f8ac58743c3dc526017894e07948de9b72052f874ee2edfdcd"
}
